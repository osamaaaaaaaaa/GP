<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.database">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.popular_hospitals"></activity>
        <activity android:name=".ui.Hospital_visit_info_submit" />
        <activity android:name=".classes.Changelang" />
        <activity android:name=".classes.edit_profile_info" />
        <activity android:name=".ui.Home_visit" />
        <activity android:name=".ui.Hospital_visit_list_view" />
        <activity android:name=".ui.Home_page" />
        <activity android:name=".ui.Register" />
        <activity android:name=".ui.Log_In" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>